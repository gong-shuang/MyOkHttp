<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.devMode" value="true" />

    <package name="default" namespace="/" extends="struts-default">
    
    <action name="login" class ="com.imooc.action.UserAction" method = "login"></action>
    
    <action name="postString" class ="com.imooc.action.UserAction" method = "postString"></action>
    
    <action name="postFile" class ="com.imooc.action.UserAction" method = "postFile"></action>
    
    <action name="uploadInfo" class ="com.imooc.action.UserAction" method = "uploadInfo"></action>

 
    </package>



    <!-- Add packages here -->

</struts>
